{
    "name": "kdn/attempts-counter",
    "description": "An abstract counter of attempts to perform some actions.",
    "type": "library",
    "keywords": [
        "attempts",
        "counter"
    ],
    "readme": "./README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Kulikov",
            "email": "kulikovdn@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/dmitry-kulikov/attempts-counter/issues",
        "source": "https://github.com/dmitry-kulikov/attempts-counter"
    },
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0 <10.0",
        "phpunit/php-invoker": ">=1.0 <4.0"
    },
    "autoload": {
        "psr-4": {
            "kdn\\attemptsCounter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kdn\\attemptsCounter\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
